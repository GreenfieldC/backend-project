{
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 50,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "parenSpacing": true,
  "jsxBracketSameLine": false,
  "semi": true,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 1000
      }
    }
  ]
}
